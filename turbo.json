{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", "dist/**"]
    },
    "build:ci": {
      "dependsOn": ["^build:ci"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**", "dist/**"]
    },
    "check": { "dependsOn": ["^check"] },
    "fmt": { "dependsOn": ["^fmt"] },
    "fmt:ci": { "dependsOn": ["^fmt:ci"] },
    "lint": { "dependsOn": ["^lint"] },
    "lint:ci": { "dependsOn": ["^lint:ci"] },
    "test": { "dependsOn": ["^test"], "inputs": ["$TURBO_DEFAULT$", ".env*"] },
    "test:coverage": {
      "dependsOn": ["^test:coverage"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["coverage/**"]
    },
    "check-types": { "dependsOn": ["^check-types"] },
    "dev": { "inputs": ["$TURBO_DEFAULT$", ".env*"], "cache": false, "persistent": true }
  }
}
