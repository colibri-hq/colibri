{
  "name": "@colibri-hq/ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "license": "MIT",
  "description": "The Colibri UI component library.",
  "keywords": [
    "colibri",
    "server",
    "ebooks",
    "ebook-management",
    "epub",
    "opds"
  ],
  "homepage": "https://github.com/colibri-hq/colibri",
  "author": {
    "name": "Moritz Mazetti",
    "email": "m@9dev.de",
    "url": "https://mazetti.me"
  },
  "repository": {
    "url": "git+https://github.com/colibri-hq/colibri.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/colibri-hq/colibri/issues"
  },
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "dev": "svelte-kit sync && svelte-package --watch",
    "build": "vite build && pnpm run prepack",
    "preview": "vite preview",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.ui.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.ui.json --watch",
    "lint": "eslint --fix --report-unused-disable-directives --report-unused-inline-configs=error --cache --cache-location=./.cache/eslint.json --cache-strategy metadata .",
    "lint:ci": "eslint --stats --report-unused-disable-directives --report-unused-inline-configs=error --cache --cache-location=./.cache/eslint.json --cache-strategy content .",
    "fmt": "prettier --write --cache --cache-location=./.cache/prettier.json --cache-strategy metadata .",
    "fmt:ci": "prettier --check --cache --cache-location=./.cache/prettier.json --cache-strategy content .",
    "storybook": "storybook dev --port 6006",
    "build-storybook": "storybook build",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run && npm run test:e2e",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@browser-logos/brave": "^3.0.13",
    "@browser-logos/chrome": "^2.0.0",
    "@browser-logos/chromium": "^2.0.1",
    "@browser-logos/edge": "^2.0.7",
    "@browser-logos/firefox": "^3.0.10",
    "@browser-logos/safari": "^2.1.0",
    "@browser-logos/safari-ios": "^1.0.15",
    "@browser-logos/silk": "^3.0.4",
    "@browser-logos/webkit": "^1.0.10",
    "@browser-logos/yandex": "^1.0.8",
    "@colibri-hq/sdk": "workspace:*",
    "@colibri-hq/tailwind": "workspace:*",
    "@fontsource-variable/material-symbols-outlined": "catalog:",
    "@fontsource/neuton": "catalog:",
    "@fontsource/titillium-web": "catalog:",
    "bits-ui": "^1.4.2",
    "blurhash": "^2.0.5",
    "esm-env": "^1.2.2",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@colibri-hq/shared": "workspace:*",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-svelte-csf": "5.0.0-next.28",
    "@storybook/blocks": "^8.6.12",
    "@storybook/svelte": "^8.6.12",
    "@storybook/sveltekit": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@sveltejs/adapter-auto": "catalog:",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "catalog:",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "catalog:",
    "@tailwindcss/vite": "catalog:",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/qrcode": "^1.5.5",
    "eslint": "catalog:",
    "eslint-plugin-storybook": "^0.11.6",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "catalog:",
    "prettier-plugin-svelte": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "publint": "^0.3.12",
    "storybook": "^8.6.12",
    "svelte": "catalog:",
    "svelte-check": "catalog:",
    "svelte-gravatar": "^1.0.3",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.20.3",
    "svelte": "^5.25.6"
  },
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "*": [
        "./dist/components/*.d.ts"
      ]
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.stories.*",
    "!dist/**/*.spec.*"
  ]
}
