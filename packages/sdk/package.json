{
  "name": "@colibri-hq/sdk",
  "version": "0.0.1",
  "description": "The Colibri SDK",
  "keywords": [
    "colibri",
    "ebook-management",
    "ebooks",
    "epub",
    "opds",
    "server"
  ],
  "homepage": "https://github.com/colibri-hq/colibri",
  "bugs": {
    "url": "https://github.com/colibri-hq/colibri/issues"
  },
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "Moritz Mazetti",
    "email": "m@9dev.de",
    "url": "https://mazetti.me"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/colibri-hq/colibri.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./schema": {
      "types": "./src/schema.d.ts",
      "import": "./src/schema.d.ts"
    },
    "./oauth": {
      "types": "./dist/oauth/index.d.ts",
      "import": "./dist/oauth/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./ebooks": {
      "types": "./dist/ebooks/index.d.ts",
      "import": "./dist/ebooks/index.js",
      "browser": "./dist/ebooks/index.js"
    },
    "./metadata": {
      "types": "./dist/metadata/index.d.ts",
      "import": "./dist/metadata/index.js",
      "browser": "./dist/metadata/index.js"
    },
    "./ingestion": {
      "types": "./dist/ingestion/index.d.ts",
      "import": "./dist/ingestion/index.js"
    },
    "./settings": {
      "types": "./dist/settings/index.d.ts",
      "import": "./dist/settings/index.js"
    },
    "./scopes": {
      "types": "./dist/scopes/index.d.ts",
      "import": "./dist/scopes/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./jobs": {
      "types": "./dist/jobs/index.d.ts",
      "import": "./dist/jobs/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist; tsc",
    "build:ci": "pnpm run build",
    "dev": "tsc --watch",
    "check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --config ./tsconfig.test.json --coverage",
    "test:ui": "vitest --config ./tsconfig.test.json --ui",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "lint": "oxlint --fix && eslint --fix --report-unused-disable-directives --report-unused-inline-configs=error --cache --cache-location=./.cache/eslint.json --cache-strategy metadata .",
    "lint:ci": "oxlint --format github && eslint --stats --report-unused-disable-directives --report-unused-inline-configs=error --cache --cache-location=./.cache/eslint.json --cache-strategy content .",
    "fmt": "oxfmt --write .",
    "fmt:ci": "oxfmt --check .",
    "prepack": "pnpm run fmt && pnpm run check && pnpm run lint && pnpm run test && pnpm run build",
    "types": "kysely-codegen --dialect postgres --env-file ../../.env --url 'env(DB_URL)' --out-file ./src/schema.d.ts --type-only-imports --default-schema public --exclude-pattern \"auth.*\" --include-pattern \"(public|authentication|vault).*\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.971.0",
    "@aws-sdk/lib-storage": "^3.971.0",
    "@aws-sdk/s3-request-presigner": "^3.971.0",
    "@colibri-hq/languages": "workspace:*",
    "@colibri-hq/mobi": "workspace:*",
    "@colibri-hq/oauth": "workspace:*",
    "@colibri-hq/open-library-client": "workspace:*",
    "@colibri-hq/pdf": "workspace:*",
    "@colibri-hq/shared": "workspace:*",
    "@simplewebauthn/server": "^13.2.2",
    "@zip.js/zip.js": "^2.8.15",
    "esm-env": "catalog:",
    "fast-xml-parser": "^5.3.3",
    "file-type": "^21.3.0",
    "he": "^1.2.0",
    "jsonwebtoken": "catalog:",
    "kysely": "^0.28.9",
    "pg": "^8.17.1",
    "turndown": "catalog:",
    "xmldom": "^0.6.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/he": "^1.2.3",
    "@types/jsonwebtoken": "catalog:",
    "@types/node": "catalog:",
    "@types/pg": "^8.16.0",
    "@types/turndown": "catalog:",
    "@types/xmldom": "^0.1.34",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "kanel": "^3.16.0",
    "kanel-kysely": "^0.8.0",
    "kysely-codegen": "^0.19.0",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "rimraf": "catalog:",
    "sharp": "^0.34.5",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
