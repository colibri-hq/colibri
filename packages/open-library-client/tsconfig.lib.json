{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "lib": [
      "DOM",
      "DOM.AsyncIterable",
      "ESNext"
    ],
    "tsBuildInfoFile": ".cache/tsbuildinfo.lib.json"
  },
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
