{
  "name": "@colibri-hq/cli",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "The command-line interface for Colibri, an ebook management app",
  "keywords": [
    "colibri",
    "server",
    "ebooks",
    "ebook-management",
    "epub",
    "opds",
    "oclif"
  ],
  "homepage": "https://github.com/colibri-hq/colibri",
  "author": {
    "name": "Moritz Mazetti",
    "email": "m@9dev.de",
    "url": "https://mazetti.me"
  },
  "repository": {
    "url": "git+https://github.com/colibri-hq/colibri.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/colibri-hq/colibri/issues"
  },
  "packageManager": "pnpm@10.6.5",
  "oclif": {
    "bin": "colibri",
    "dirname": "colibri",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "topics": {
      "users": {
        "description": "Manage users"
      },
      "oauth": {
        "description": "Manage OAuth integrations"
      }
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "postbuild": "pnpx tsc-alias",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "test": "vitest --config tsconfig.test.json",
    "test:coverage": "vitest run --config tsconfig.test.json --coverage",
    "test:ui": "vitest --config tsconfig.test.json --ui",
    "version": "oclif readme && git add README.md",
    "check": "tsc --noEmit",
    "lint": "eslint --fix --report-unused-disable-directives --report-unused-inline-configs=error --cache --cache-location=./.cache/eslint.json --cache-strategy metadata .",
    "lint:ci": "eslint --stats --report-unused-disable-directives --report-unused-inline-configs=error --cache --cache-location=./.cache/eslint.json --cache-strategy content .",
    "fmt": "prettier --write --cache --cache-location=./.cache/prettier.json --cache-strategy metadata .",
    "fmt:ci": "prettier --check --cache --cache-location=./.cache/prettier.json --cache-strategy content .",
    "prepack": "pnpm run fmt && pnpm run check && pnpm run lint && pnpm run package",
    "package": "pnpm run build && pnpm run oclif manifest && oclif readme"
  },
  "dependencies": {
    "@colibri-hq/oauth": "workspace:*",
    "@colibri-hq/sdk": "workspace:*",
    "@colibri-hq/shared": "workspace:*",
    "@inquirer/prompts": "^7.5.0",
    "@oclif/core": "^4.3.0",
    "@oclif/plugin-help": "^6.2.28",
    "@oclif/plugin-not-found": "^3.2.50",
    "@oclif/plugin-plugins": "^5.4.36",
    "ansis": "^3.17.0",
    "open": "^10.1.2",
    "ora": "^8.2.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.12",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "catalog:",
    "eslint-config-oclif": "^6.0.50",
    "eslint-config-prettier": "^10.1.2",
    "oclif": "^4.17.46",
    "prettier": "catalog:",
    "ts-node": "^10.9.2",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "bin": {
    "colibri": "./bin/run.js"
  }
}
